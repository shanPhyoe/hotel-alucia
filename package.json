{
    "name": "alucia",
    "version": "1.0.0",
    "description": "Deluxe Hotel in Bali",
    "main": "index.html",
    "scripts": {
        "watch:sass": "node-sass sass/main.scss styles/styles.css -w",
        "compile:sass": "node-sass sass/main.scss css/styles.compile.css",
        "prefix:css": "postcss css/styles.compile.css --use autoprefixer --output css/styles.prefix.css",
        "compress:css": "node-sass css/style.prefix.css styles/styles.css --output-style compressed",
        "build:css": "npm-run-all compile:sass prefix:css compress:css"
    },
    "keywords": [
        "alutia"
    ],
    "author": "shanPhyoe",
    "license": "ISC",
    "devDependencies": {
        "autoprefixer": "^10.4.4",
        "grunt-svgstore": "^2.0.0",
        "node-sass": "^7.0.1",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^9.1.0"
    }
}
